generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model additional_fees {
  id          Int      @default(autoincrement()) @id
  sale_id     Int
  name        String?
  description String?
  amount      Float?
  created_at  DateTime
  updated_at  DateTime
}

model addresses {
  id                    Int      @default(autoincrement()) @id
  full_name             String?
  line_one              String?
  line_two              String?
  city                  String?
  state_province_region String?
  postal_code           String?
  country               String?
  company_id            Int
  created_at            DateTime
  updated_at            DateTime
  description           String?
  point_of_contact_info String?
  is_billing_address    Boolean?

  @@index([company_id], name: "index_addresses_on_company_id")
}

model admin_invoices {
  id               Int      @default(autoincrement()) @id
  sale_id          Int?
  payment_info     String?
  transaction_info String?
  buyer_info       String?
  buyer_email      String?
  cc_emails        String?
  created_at       DateTime
  updated_at       DateTime
  sales            sales?   @relation(fields: [sale_id], references: [id])

  @@index([sale_id], name: "index_admin_invoices_on_sale_id")
}

model announcements {
  id                 Int      @default(autoincrement()) @id
  title              String?
  body               String?
  media              String?
  icon               String?
  created_by_user_id Int?
  created_at         DateTime
  updated_at         DateTime
}

model api_keys {
  id           Int      @default(autoincrement()) @id
  access_token String
  description  String?
  created_at   DateTime
  updated_at   DateTime
  company_id   Int
}

model ar_internal_metadata {
  key        String   @id
  value      String?
  created_at DateTime
  updated_at DateTime
}

model asset_attachments {
  id              Int            @default(autoincrement()) @id
  attachment      String?
  attachment_type String?
  asset_record_id Int?
  created_at      DateTime
  updated_at      DateTime
  asset_records   asset_records? @relation(fields: [asset_record_id], references: [id])

  @@index([asset_record_id], name: "index_asset_attachments_on_asset_record_id")
}

model asset_images {
  id              Int      @default(autoincrement()) @id
  image           String?
  asset_record_id Int?
  created_at      DateTime
  updated_at      DateTime
}

model asset_records {
  id                            Int                          @default(autoincrement()) @id
  sku                           String?
  model                         String?
  created_at                    DateTime
  updated_at                    DateTime
  user_id                       Int
  category                      String?                      @default("none")
  manufacturer                  String?
  quantity                      Int                          @default(0)
  condition                     String?
  original_purchase_price       Float?
  images                        String[]
  minimum_offer_amount_per_unit Float?
  buy_now_amount                Float?                       @default(0)
  country                       String?
  listing_period_start          DateTime?
  listing_period_end            DateTime?
  description                   String?
  name                          String?
  cert_links                    String[]
  catalog_links                 String[]
  spec_links                    String[]
  status                        Int?                         @default(0)
  team_id                       Int?
  bulk_upload_id                Int?
  currency                      String?                      @default("USD")
  sale_id                       Int?
  company_id                    Int
  subcategory                   String?                      @default("none")
  additional_fields             Json                         @default("{}")
  package_id                    Int?
  seller_terms                  String?
  internal_marketplace_id       Int?
  listed_at                     DateTime?
  shipping_dimensions           String?
  shipping_weight               String?
  address_id                    Int?
  // This type is currently not supported.
  // search_vector              textSearch?
  units                         String?
  size1                         String?
  size2                         String?
  state_province                String?
  location                      String?
  lead_time                     String?
  packaged                      String?
  is_hazmat                     String?
  msds_links                    String?
  spare_part                    Boolean?                     @default(false)
  last_offer                    Float?
  uuid                          String?
  sale_method                   Int                          @default(2)
  buy_now_enabled               Boolean?                     @default(false)
  city                          String?
  state                         String?
  un_segment_code               Int?
  un_segment                    String?
  un_family_code                Int?
  un_family                     String?
  un_class_code                 Int?
  un_class                      String?
  un_commodity_code             Int?
  un_commodity                  String?
  inventory_type                String?
  un_categories                 Json?
  show_minimum_offer_amount     Boolean?                     @default(false)
  companies                     companies                    @relation(fields: [company_id], references: [id])
  internal_marketplaces         internal_marketplaces?       @relation(fields: [internal_marketplace_id], references: [id])
  users                         users                        @relation(fields: [user_id], references: [id])
  asset_attachments             asset_attachments[]
  // offer_asset_records        offer_asset_records[]
  requisition_items             requisition_items[]
  user_watched_asset_records    user_watched_asset_records[]

  @@index([address_id], name: "index_asset_records_on_address_id")
  @@index([bulk_upload_id], name: "index_asset_records_on_bulk_upload_id")
  @@index([company_id], name: "index_asset_records_on_company_id")
  @@index([country], name: "index_asset_records_on_country")
  @@index([internal_marketplace_id], name: "index_asset_records_on_internal_marketplace_id")
  @@index([sale_id], name: "index_asset_records_on_sale_id")
  @@index([status], name: "index_asset_records_on_status")
  @@index([team_id], name: "index_asset_records_on_team_id")
  @@index([user_id], name: "index_asset_records_on_user_id")
}

model auctions {
  id               Int       @default(autoincrement()) @id
  start_datetime   DateTime?
  end_datetime     DateTime?
  company_id       Int?
  user_id          Int?
  auto_extend      Int?
  created_at       DateTime
  updated_at       DateTime
  start_price      Float?
  reserve_price    Float?
  auctionable_id   Int?
  auctionable_type String?
  unlisted         Boolean?
  bid_increment    Int?      @default(100)
}

model banners {
  id            Int      @default(autoincrement()) @id
  header        String?
  icon          String?
  tagline       String?
  button_text   String?
  button_link   String?
  display_order Int?
  active        Boolean? @default(false)
  created_at    DateTime
  updated_at    DateTime
}

model bids {
  id         Int        @default(autoincrement()) @id
  company_id Int?
  created_at DateTime
  updated_at DateTime
  user_id    Int?
  amount     Float?
  auction_id Int?
  companies  companies? @relation(fields: [company_id], references: [id])

  @@index([company_id], name: "index_bids_on_company_id")
  @@index([user_id], name: "index_bids_on_user_id")
}

model bulk_uploads {
  id                  Int        @default(autoincrement()) @id
  upload_type         String?
  user_id             Int?
  team_id             Int?
  created_at          DateTime
  updated_at          DateTime
  upload_number       Int?
  description         String?
  asset_records_count Int?
  company_id          Int?
  uuid                String?
  companies           companies? @relation(fields: [company_id], references: [id])
  teams               teams?     @relation(fields: [team_id], references: [id])
  users               users?     @relation(fields: [user_id], references: [id])

  @@index([company_id], name: "index_bulk_uploads_on_company_id")
  @@index([team_id], name: "index_bulk_uploads_on_team_id")
  @@index([user_id], name: "index_bulk_uploads_on_user_id")
}

model buyer_hse_observations {
  id                          Int                        @default(autoincrement()) @id
  buyer_hse_recommendation_id Int?
  body                        String?
  created_at                  DateTime
  updated_at                  DateTime
  buyer_hse_recommendations   buyer_hse_recommendations? @relation(fields: [buyer_hse_recommendation_id], references: [id])

  @@index([buyer_hse_recommendation_id], name: "index_buyer_hse_observations_on_buyer_hse_recommendation_id")
}

model buyer_hse_recommendations {
  id                                                      Int                                            @default(autoincrement()) @id
  supplier_registration_form_id                           Int?
  company_id                                              Int?
  status                                                  String?                                        @default("new")
  supplier_cleared_to_proceed                             Boolean?                                       @default(false)
  supplier_concern_to_proceed                             Boolean?                                       @default(false)
  supplier_prequalification_comments                      String?
  supplier_not_suitable                                   Boolean?                                       @default(false)
  hse_assessor_name                                       String?
  field_verficiation_carried_out                          Boolean?                                       @default(false)
  assessor_name                                           String?
  reviewer_name                                           String?
  associated_csg_hydrocarbon_upstream                     Boolean?                                       @default(false)
  associated_csg_hydrocarbon_downstream                   Boolean?                                       @default(false)
  associated_csg_power_non_nuclear                        Boolean?                                       @default(false)
  associated_csg_power_nuclear                            Boolean?                                       @default(false)
  associated_csg_mining_and_metals                        Boolean?                                       @default(false)
  associated_csg_infrastructure                           Boolean?                                       @default(false)
  party_screening_denied_status                           Boolean?                                       @default(false)
  party_screening_clear_status                            Boolean?                                       @default(false)
  date_of_last_check                                      DateTime?
  export_restrictions                                     Boolean?                                       @default(false)
  distribution_procurement_to_attach_in_supplier_database Boolean?                                       @default(false)
  created_at                                              DateTime
  updated_at                                              DateTime
  hse_recommendation_date                                 DateTime?
  location_of_assessment                                  String?
  date_of_assessment                                      DateTime?
  approve_reject_date                                     DateTime?
  ethics_policies_assessment                              String?
  ethics_policies_comments                                String?
  company_profile_codes_and_standards_form_id             Int?
  company_profile_hse_form_id                             Int?
  ethics_due_diligence_assessment                         String?                                        @default("Not Evaluated")
  ethics_due_diligence_comments                           String?
  ethics_overall_assessment                               String?
  ethics_overall_comments                                 String?
  ethics_approving_reviewer                               String?
  ethics_approval_date                                    DateTime?
  general_ethics_approved                                 Boolean?                                       @default(false)
  companies                                               companies?                                     @relation(fields: [company_id], references: [id])
  company_profile_codes_and_standards_forms               company_profile_codes_and_standards_forms?     @relation(fields: [company_profile_codes_and_standards_form_id], references: [id])
  company_profile_hse_forms                               company_profile_hse_forms?                     @relation(fields: [company_profile_hse_form_id], references: [id])
  supplier_registration_forms                             supplier_registration_forms?                   @relation(fields: [supplier_registration_form_id], references: [id])
  buyer_hse_observations                                  buyer_hse_observations[]
  ethics_compliance_report_uploads                        ethics_compliance_report_uploads[]
  supplier_registration_form_feedback_comments            supplier_registration_form_feedback_comments[]

  @@index([company_profile_codes_and_standards_form_id], name: "idx_buyer_hse_recommendations")
  @@index([supplier_registration_form_id], name: "idx_supplier_buyer_hse_rec_reg_form")
  @@index([company_id], name: "index_buyer_hse_recommendations_on_company_id")
  @@index([company_profile_hse_form_id], name: "index_buyer_hse_recommendations_on_company_profile_hse_form_id")
  @@unique([company_id, supplier_registration_form_id], name: "index_company_id_supplier_registration_form_id")
}

model buyer_proof_of_payments {
  id             Int      @default(autoincrement()) @id
  sale_id        Int?
  bank_name      String?
  account_number String?
  total_sent     Float?
  notes          String?
  attachment     String?
  created_at     DateTime
  updated_at     DateTime
  sales          sales?   @relation(fields: [sale_id], references: [id])

  @@index([sale_id], name: "index_buyer_proof_of_payments_on_sale_id")
}

model buyer_suppliers {
  id                                                       Int        @default(autoincrement()) @id
  supplier_company_id                                      Int?
  buyer_company_id                                         Int?
  created_at                                               DateTime
  updated_at                                               DateTime
  companies_buyer_suppliers_buyer_company_idTocompanies    companies? @relation("buyer_suppliers_buyer_company_idTocompanies", fields: [buyer_company_id], references: [id])
  companies_buyer_suppliers_supplier_company_idTocompanies companies? @relation("buyer_suppliers_supplier_company_idTocompanies", fields: [supplier_company_id], references: [id])

  @@index([buyer_company_id], name: "index_buyer_suppliers_on_buyer_company_id")
  @@index([supplier_company_id], name: "index_buyer_suppliers_on_supplier_company_id")
}

model categories {
  id            Int             @default(autoincrement()) @id
  name          String
  created_at    DateTime
  updated_at    DateTime
  subcategories subcategories[]
}

model checkouts {
  id                       Int      @default(autoincrement()) @id
  sale_id                  Int
  created_at               DateTime
  updated_at               DateTime
  submitted_payment        Boolean? @default(false)
  received_goods           Boolean? @default(false)
  released_payment         Boolean? @default(false)
  shipping_address         String?
  shipping_details         String?
  saved_shipping_info      Boolean  @default(false)
  completed_payment        Boolean  @default(false)
  shipment_marked_received Boolean  @default(false)
  wire_confirmation        String?
  details_for_seller       String?
  payment_marked_received  Boolean? @default(false)
  status                   Int?     @default(0)
  sales                    sales    @relation(fields: [sale_id], references: [id])

  @@index([sale_id], name: "index_checkouts_on_sale_id")
}

model codes_and_standards_certificate_uploads {
  id                                          Int                          @default(autoincrement()) @id
  supplier_registration_form_id               Int?
  attachment                                  String?
  created_at                                  DateTime
  updated_at                                  DateTime
  company_id                                  Int?
  company_profile_codes_and_standards_form_id Int?
  supplier_registration_forms                 supplier_registration_forms? @relation(fields: [supplier_registration_form_id], references: [id])

  @@index([company_profile_codes_and_standards_form_id], name: "idx_certificate_uploads")
  @@index([supplier_registration_form_id], name: "idx_codes_and_standards_certificate_uploads")
  @@index([company_id], name: "index_codes_and_standards_certificate_uploads_on_company_id")
}

model codes_and_standards_other_doc_uploads {
  id                                          Int                          @default(autoincrement()) @id
  supplier_registration_form_id               Int?
  attachment                                  String?
  created_at                                  DateTime
  updated_at                                  DateTime
  company_id                                  Int?
  company_profile_codes_and_standards_form_id Int?
  supplier_registration_forms                 supplier_registration_forms? @relation(fields: [supplier_registration_form_id], references: [id])

  @@index([supplier_registration_form_id], name: "idx_codes_and_standards_other_doc_uploads")
  @@index([company_profile_codes_and_standards_form_id], name: "idx_other_doc_uploads")
  @@index([company_id], name: "index_codes_and_standards_other_doc_uploads_on_company_id")
}

model commercial_evaluation_bid_summaries {
  id            Int      @default(autoincrement()) @id
  rfq_id        Int
  rfq_bid_id    Int
  summary_notes String?
  created_at    DateTime
  updated_at    DateTime
  acceptable    Boolean? @default(false)
  rfq_bids      rfq_bids @relation(fields: [rfq_bid_id], references: [id])
  rfqs          rfqs     @relation(fields: [rfq_id], references: [id])

  @@index([rfq_bid_id], name: "index_commercial_evaluation_bid_summaries_on_rfq_bid_id")
  @@index([rfq_id], name: "index_commercial_evaluation_bid_summaries_on_rfq_id")
}

model commercial_questions {
  id                   Int                    @default(autoincrement()) @id
  rfq_id               Int
  company_id           Int
  user_id              Int
  question_type        Int                    @default(0)
  content              String
  header               String?
  subheader            String?
  created_at           DateTime
  updated_at           DateTime
  companies            companies              @relation(fields: [company_id], references: [id])
  rfqs                 rfqs                   @relation(fields: [rfq_id], references: [id])
  users                users                  @relation(fields: [user_id], references: [id])
  commercial_responses commercial_responses[]

  @@index([company_id], name: "index_commercial_questions_on_company_id")
  @@index([rfq_id], name: "index_commercial_questions_on_rfq_id")
  @@index([user_id], name: "index_commercial_questions_on_user_id")
}

model commercial_responses {
  id                                Int                              @default(autoincrement()) @id
  rfq_id                            Int
  rfq_bid_id                        Int
  company_id                        Int
  commercial_question_id            Int
  user_id                           Int?
  content                           String?
  accepted                          Boolean?
  feedback                          String?
  created_at                        DateTime
  updated_at                        DateTime
  rfq_communication_thread_group_id Int?
  commercial_questions              commercial_questions             @relation(fields: [commercial_question_id], references: [id])
  companies                         companies                        @relation(fields: [company_id], references: [id])
  rfq_bids                          rfq_bids                         @relation(fields: [rfq_bid_id], references: [id])
  rfq_communication_thread_groups   rfq_communication_thread_groups? @relation(fields: [rfq_communication_thread_group_id], references: [id])
  rfqs                              rfqs                             @relation(fields: [rfq_id], references: [id])
  users                             users?                           @relation(fields: [user_id], references: [id])

  @@index([commercial_question_id], name: "index_commercial_responses_on_commercial_question_id")
  @@index([company_id], name: "index_commercial_responses_on_company_id")
  @@index([rfq_bid_id], name: "index_commercial_responses_on_rfq_bid_id")
  @@index([rfq_communication_thread_group_id], name: "index_commercial_responses_on_rfq_communication_thread_group_id")
  @@index([rfq_id], name: "index_commercial_responses_on_rfq_id")
  @@index([user_id], name: "index_commercial_responses_on_user_id")
  @@unique([rfq_bid_id, commercial_question_id], name: "index_commercial_response_uniqueness")
}

model companies {
  id                                                                                   Int                                         @default(autoincrement()) @id
  name                                                                                 String                                      @unique
  country                                                                              String?
  industry                                                                             String?
  created_at                                                                           DateTime
  updated_at                                                                           DateTime
  approved_by_global_admin                                                             Boolean                                     @default(false)
  banking_account_number                                                               String?
  banking_routing_number                                                               String?
  phone_number                                                                         String?
  fax_number                                                                           String?
  bank_name                                                                            String?
  screen_name                                                                          String?                                     @default("Private Company")
  slug_name                                                                            String
  sale_transaction_fee                                                                 Float                                       @default(12.5)
  profile_pic                                                                          String?
  profile_copy                                                                         String?
  annual_revenue                                                                       String?
  product_certifications                                                               String[]
  business_type                                                                        String?
  certifications                                                                       String[]
  year_established                                                                     Int?
  company_type                                                                         String[]
  procure_transaction_fee                                                              Float?                                      @default(0)
  parent_company_name                                                                  String?
  website                                                                              String?
  users_count                                                                          Int?
  salesforce_accounts_id                                                               Int?
  salesforce_accounts_companies_salesforce_accounts_idTosalesforce_accounts            salesforce_accounts?                        @relation("companies_salesforce_accounts_idTosalesforce_accounts", fields: [salesforce_accounts_id], references: [id])
  asset_records                                                                        asset_records[]
  bids                                                                                 bids[]
  bulk_uploads                                                                         bulk_uploads[]
  buyer_hse_recommendations                                                            buyer_hse_recommendations[]
  buyer_suppliers_buyer_suppliers_buyer_company_idTocompanies                          buyer_suppliers[]                           @relation("buyer_suppliers_buyer_company_idTocompanies")
  buyer_suppliers_buyer_suppliers_supplier_company_idTocompanies                       buyer_suppliers[]                           @relation("buyer_suppliers_supplier_company_idTocompanies")
  commercial_questions                                                                 commercial_questions[]
  commercial_responses                                                                 commercial_responses[]
  company_banks                                                                        company_banks[]
  company_contacts_companiesTocompany_contacts_company_id                              company_contacts[]                          @relation("companiesTocompany_contacts_company_id")
  company_contacts_companiesTocompany_contacts_contact_id                              company_contacts[]                          @relation("companiesTocompany_contacts_contact_id")
  company_profile_codes_and_standards_forms                                            company_profile_codes_and_standards_forms[]
  company_profile_hse_forms                                                            company_profile_hse_forms[]
  company_registration_forms                                                           company_registration_forms?
  company_relationships_companiesTocompany_relationships_child_id                      company_relationships[]                     @relation("companiesTocompany_relationships_child_id")
  company_relationships_companiesTocompany_relationships_parent_id                     company_relationships[]                     @relation("companiesTocompany_relationships_parent_id")
  company_settings                                                                     company_settings?
  company_signup_links                                                                 company_signup_links[]
  project_commercial_questions                                                         project_commercial_questions[]
  project_members                                                                      project_members[]
  projects                                                                             projects[]
  relationship_requests_companiesTorelationship_requests_acceptor_id                   relationship_requests[]                     @relation("companiesTorelationship_requests_acceptor_id")
  relationship_requests_companiesTorelationship_requests_requestor_id                  relationship_requests[]                     @relation("companiesTorelationship_requests_requestor_id")
  requisition_comments                                                                 requisition_comments[]
  requisition_item_comments                                                            requisition_item_comments[]
  requisitions_companiesTorequisitions_buyer_company_id                                requisitions[]                              @relation("companiesTorequisitions_buyer_company_id")
  requisitions_companiesTorequisitions_seller_company_id                               requisitions[]                              @relation("companiesTorequisitions_seller_company_id")
  rfq_audit_log_entries                                                                rfq_audit_log_entries[]
  rfq_bid_itemized_prices                                                              rfq_bid_itemized_prices[]
  rfq_commercial_docs                                                                  rfq_commercial_docs[]
  rfq_communication_threads_companiesTorfq_communication_threads_rfq_buyer_company_id  rfq_communication_threads[]                 @relation("companiesTorfq_communication_threads_rfq_buyer_company_id")
  rfq_communication_threads_companiesTorfq_communication_threads_rfq_vendor_company_id rfq_communication_threads[]                 @relation("companiesTorfq_communication_threads_rfq_vendor_company_id")
  rfq_deliverable_items                                                                rfq_deliverable_items[]
  rfq_messages_companiesTorfq_messages_rfq_company_message_receiver_id                 rfq_messages[]                              @relation("companiesTorfq_messages_rfq_company_message_receiver_id")
  rfq_messages_companiesTorfq_messages_rfq_company_message_sender_id                   rfq_messages[]                              @relation("companiesTorfq_messages_rfq_company_message_sender_id")
  rfq_technical_docs                                                                   rfq_technical_docs[]
  rfqs                                                                                 rfqs[]
  sales_companiesTosales_buyer_company_id                                              sales[]                                     @relation("companiesTosales_buyer_company_id")
  sales_companiesTosales_seller_company_id                                             sales[]                                     @relation("companiesTosales_seller_company_id")
  salesforce_accounts_companiesTosalesforce_accounts_company_id                        salesforce_accounts[]                       @relation("companiesTosalesforce_accounts_company_id")
  shopping_lists                                                                       shopping_lists[]
  subscriptions                                                                        subscriptions[]
  supplier_profiles                                                                    supplier_profiles[]
  supplier_qualifications                                                              supplier_qualifications[]
  supplier_registration_forms                                                          supplier_registration_forms[]
  technical_questions                                                                  technical_questions[]
  technical_responses                                                                  technical_responses[]

  @@index([salesforce_accounts_id], name: "index_companies_on_salesforce_accounts_id")
}

model companies_internal_marketplaces {
  internal_marketplace_id Int
  company_id              Int

  @@index([internal_marketplace_id], name: "idx_companies_internal_marketplaces_on_internal_marketplace_id")
  @@index([company_id], name: "index_companies_internal_marketplaces_on_company_id")
  @@unique([company_id, internal_marketplace_id], name: "index_unique_company_id_and_internal_marketplace_id")
}

model companies_verticals {
  id          Int  @default(autoincrement()) @id
  company_id  Int?
  vertical_id Int?
}

model company_banks {
  id             Int        @default(autoincrement()) @id
  name           String?
  address        String?
  contact        String?
  routing_number String?
  account_number String?
  company_id     Int?
  created_at     DateTime
  updated_at     DateTime
  companies      companies? @relation(fields: [company_id], references: [id])

  @@index([company_id], name: "index_company_banks_on_company_id")
