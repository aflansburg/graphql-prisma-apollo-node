  users               users                 @relation(fields: [user_id], references: [id])
  technical_responses technical_responses[]

  @@index([company_id], name: "index_technical_questions_on_company_id")
  @@index([rfq_id], name: "index_technical_questions_on_rfq_id")
  @@index([user_id], name: "index_technical_questions_on_user_id")
